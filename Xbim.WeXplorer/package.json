{
  "name": "xbim-webui",
  "version": "0.1.41",
  "devDependencies": {
    "dts-bundle": "^0.7.2",
    "jsdoc": "3.3.0",
    "webpack": "^2.4.1",
    "worker-loader": "^0.8.0",
    "@types/node": "7.0.18",
    "ts-loader": "2.0.3",
    "typescript": "2.3.2"
  },
  "files": [
    "Browser/*",
    "Build/*",
    "Plugins/*",
    "Viewer/*",
    "Libs/*",
    "*.js*",
    "*.ts",
    "*.d.ts"
  ],
  "scripts": {
    "jsdoc": "jsdoc -c jsdoc.config.json",
    "tsc": "tsc",
    "webpack:dev": "SET NODE_ENV=development&& webpack -d",
    "webpack:prod": "SET NODE_ENV=production&& webpack -p",
    "build": "npm run jsdoc && npm run webpack:dev && npm run webpack:prod"
  }
}
